	.FUNCT	CIRCUIT-BREAKER-F,V
	EQUAL?	PRSA,V?EXAMINE,V?ANALYZE \?ELS5
	FSET?	PRSO,OPENBIT \?ELS5
	CALL	TELL-HINT,31,ELECTRICAL-CONTROL-PANEL,0
	RSTACK	
