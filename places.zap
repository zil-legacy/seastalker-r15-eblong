	.FUNCT	ELECTRICAL-CONTROL-PANEL-F
	EQUAL?	PRSA,V?EXAMINE,V?ANALYZE \?ELS5
	FSET?	CIRCUIT-BREAKER,OPENBIT \?ELS5
	CALL	THIS-IS-IT,CIRCUIT-BREAKER
	PRINTI	"A "
